{
  "id": "{{prefix}}einkuenfte",
  "initial": "start",
  "states": {
    "start": {
      "on": {
        "SUBMIT": "{{prefix}}staatliche-leistungen",
        "BACK": "#antragStart"
      }
    },
    "{{prefix}}staatliche-leistungen": {
      "on": {
        "SUBMIT": [
          {
            "guard": "{{prefix}}staatlicheLeistungenIsBuergergeld",
            "target": "{{prefix}}buergergeld"
          },
          {
            "guard": "{{prefix}}staatlicheLeistungenIsArbeitslosengeld",
            "target": "{{prefix}}arbeitslosengeld"
          },
          {
            "guard": "{{prefix}}staatlicheLeistungenIsKeine",
            "target": "{{prefix}}einkommen"
          },
          "#abgabe"
        ],
        "BACK": "start"
      }
    },
    "{{prefix}}buergergeld": {
      "id": "{{prefix}}buergergeld",
      "on": {
        "SUBMIT": "{{prefix}}einkommen",
        "BACK": "{{prefix}}staatliche-leistungen"
      }
    },
    "{{prefix}}arbeitslosengeld": {
      "id": "{{prefix}}arbeitslosengeld",
      "on": {
        "SUBMIT": "{{prefix}}einkommen",
        "BACK": "{{prefix}}staatliche-leistungen"
      }
    },
    "{{prefix}}einkommen": {
      "id": "{{prefix}}einkommen",
      "initial": "{{prefix}}erwerbstaetig",
      "states": {
        "{{prefix}}erwerbstaetig": {
          "id": "{{prefix}}erwerbstaetig",
          "on": {
            "SUBMIT": [
              {
                "guard": "{{prefix}}notEmployed",
                "target": "#{{prefix}}rente-frage"
              },
              "{{prefix}}art"
            ],
            "BACK": [
              {
                "guard": "{{prefix}}staatlicheLeistungenIsBuergergeld",
                "target": "#{{prefix}}einkuenfte.{{prefix}}buergergeld"
              },
              {
                "guard": "{{prefix}}staatlicheLeistungenIsArbeitslosengeld",
                "target": "#{{prefix}}einkuenfte.{{prefix}}arbeitslosengeld"
              },
              "#{{prefix}}einkuenfte.{{prefix}}staatliche-leistungen"
            ]
          }
        },
        "{{prefix}}art": {
          "id": "{{prefix}}art",
          "on": {
            "SUBMIT": [
              {
                "guard": "{{prefix}}isEmployee",
                "target": "{{prefix}}netto-einkommen"
              },
              "{{prefix}}selbststaendig"
            ],
            "BACK": "{{prefix}}erwerbstaetig"
          }
        },
        "{{prefix}}netto-einkommen": {
          "id": "{{prefix}}netto-einkommen",
          "on": {
            "SUBMIT": [
              {
                "guard": "{{prefix}}isSelfEmployed",
                "target": "{{prefix}}selbststaendig"
              },
              "#{{prefix}}einkuenfte.{{prefix}}abzuege"
            ],
            "BACK": "{{prefix}}art"
          }
        },
        "{{prefix}}selbststaendig": {
          "id": "{{prefix}}selbststaendig",
          "on": {
            "SUBMIT": "{{prefix}}selbststaendig-abzuege",
            "BACK": [
              {
                "guard": "{{prefix}}isEmployee",
                "target": "{{prefix}}netto-einkommen"
              },
              "#{{prefix}}einkommen.{{prefix}}art"
            ]
          }
        },
        "{{prefix}}selbststaendig-abzuege": {
          "id": "{{prefix}}selbststaendig-abzuege",
          "on": {
            "BACK": "{{prefix}}selbststaendig",
            "SUBMIT": "#{{prefix}}einkuenfte.{{prefix}}abzuege"
          }
        }
      }
    },
    "{{prefix}}abzuege": {
      "id": "{{prefix}}abzuege",
      "initial": "{{prefix}}arbeitsweg",
      "states": {
        "{{prefix}}arbeitsweg": {
          "id": "{{prefix}}arbeitsweg",
          "on": {
            "SUBMIT": [
              {
                "guard": "{{prefix}}usesPublicTransit",
                "target": "{{prefix}}opnv-kosten"
              },
              {
                "guard": "{{prefix}}usesPrivateVehicle",
                "target": "{{prefix}}arbeitsplatz-entfernung"
              },
              {
                "guard": "{{prefix}}commuteMethodPlaysNoRole",
                "target": "{{prefix}}keine-rolle"
              },
              "{{prefix}}arbeitsausgaben"
            ],
            "BACK": [
              {
                "guard": "{{prefix}}isSelfEmployed",
                "target": "#{{prefix}}einkommen.{{prefix}}selbststaendig-abzuege"
              },
              "#{{prefix}}einkommen.{{prefix}}netto-einkommen"
            ]
          }
        },
        "{{prefix}}opnv-kosten": {
          "id": "{{prefix}}opnv-kosten",
          "on": {
            "SUBMIT": "{{prefix}}arbeitsplatz-entfernung",
            "BACK": "{{prefix}}arbeitsweg"
          }
        },
        "{{prefix}}arbeitsplatz-entfernung": {
          "id": "{{prefix}}arbeitsplatz-entfernung",
          "on": {
            "SUBMIT": "{{prefix}}arbeitsausgaben",
            "BACK": [
              {
                "guard": "{{prefix}}usesPublicTransit",
                "target": "{{prefix}}opnv-kosten"
              },
              "{{prefix}}arbeitsweg"
            ]
          }
        },
        "{{prefix}}keine-rolle": {
          "on": {
            "SUBMIT": "{{prefix}}arbeitsausgaben",
            "BACK": "{{prefix}}arbeitsweg"
          }
        },
        "{{prefix}}arbeitsausgaben": {
          "id": "{{prefix}}arbeitsausgaben",
          "initial": "{{prefix}}arbeitsausgaben-frage",
          "states": {
            "{{prefix}}arbeitsausgaben-frage": {
              "id": "{{prefix}}arbeitsausgaben-frage",
              "on": {
                "SUBMIT": [
                  {
                    "guard": "{{prefix}}hasAndereArbeitsausgaben",
                    "target": "{{prefix}}uebersicht"
                  },
                  "#{{prefix}}einkuenfte.{{prefix}}rente-frage"
                ],
                "BACK": [
                  {
                    "guard": "{{prefix}}usesPublicTransit",
                    "target": "#{{prefix}}abzuege.{{prefix}}arbeitsplatz-entfernung"
                  },
                  {
                    "guard": "{{prefix}}usesPrivateVehicle",
                    "target": "#{{prefix}}abzuege.{{prefix}}arbeitsplatz-entfernung"
                  },
                  {
                    "guard": "{{prefix}}commuteMethodPlaysNoRole",
                    "target": "#{{prefix}}abzuege.{{prefix}}keine-rolle"
                  },
                  "#{{prefix}}abzuege.{{prefix}}arbeitsweg"
                ]
              }
            },
            "{{prefix}}uebersicht": {
              "id": "{{prefix}}uebersicht",
              "on": {
                "SUBMIT": [
                  {
                    "guard": "{{prefix}}hasAndereArbeitsausgabenAndEmptyArray",
                    "target": "{{prefix}}warnung"
                  },
                  "#{{prefix}}einkuenfte.{{prefix}}rente-frage"
                ],
                "BACK": "{{prefix}}arbeitsausgaben-frage",
                "{{prefix}}add-arbeitsausgaben": {
                  "guard": "{{prefix}}isValidArbeitsausgabenArrayIndex",
                  "target": "{{prefix}}arbeitsausgabe"
                }
              }
            },
            "{{prefix}}warnung": {
              "on": {
                "BACK": "{{prefix}}uebersicht",
                "SUBMIT": "#{{prefix}}einkuenfte.{{prefix}}rente-frage"
              }
            },
            "{{prefix}}arbeitsausgabe": {
              "initial": "{{prefix}}daten",
              "states": {
                "{{prefix}}daten": {
                  "on": {
                    "BACK": "#{{prefix}}arbeitsausgaben.{{prefix}}uebersicht",
                    "SUBMIT": "#{{prefix}}arbeitsausgaben.{{prefix}}uebersicht"
                  }
                }
              }
            }
          }
        }
      }
    },
    "{{prefix}}rente-frage": {
      "id": "{{prefix}}rente-frage",
      "on": {
        "SUBMIT": [
          {
            "guard": "{{prefix}}receivesPension",
            "target": "{{prefix}}rente"
          },
          "{{prefix}}unterhalt-frage"
        ],
        "BACK": [
          {
            "guard": "{{prefix}}notEmployed",
            "target": "{{prefix}}einkommen"
          },
          {
            "guard": "{{prefix}}hasAndereArbeitsausgaben",
            "target": "#{{prefix}}abzuege.{{prefix}}arbeitsausgaben.{{prefix}}uebersicht"
          },
          "#{{prefix}}abzuege.{{prefix}}arbeitsausgaben"
        ]
      }
    },
    "{{prefix}}rente": {
      "on": {
        "SUBMIT": "{{prefix}}unterhalt-frage",
        "BACK": "{{prefix}}rente-frage"
      }
    },
    "{{prefix}}unterhalt-frage": {
      "id": "{{prefix}}unterhalt-frage",
      "on": {
        "SUBMIT": [
          {
            "guard": "{{prefix}}receivesSupport",
            "target": "{{prefix}}unterhalt"
          },
          "{{prefix}}leistungen"
        ],
        "BACK": [
          {
            "guard": "{{prefix}}receivesPension",
            "target": "{{prefix}}rente"
          },
          "{{prefix}}rente-frage"
        ]
      }
    },
    "{{prefix}}unterhalt": {
      "id": "{{prefix}}unterhalt",
      "on": {
        "SUBMIT": "{{prefix}}leistungen",
        "BACK": "{{prefix}}unterhalt-frage"
      }
    },
    "{{prefix}}leistungen": {
      "id": "{{prefix}}leistungen",
      "initial": "{{prefix}}frage",
      "states": {
        "{{prefix}}frage": {
          "on": {
            "SUBMIT": [
              {
                "guard": "{{prefix}}hasWohngeld",
                "target": "{{prefix}}wohngeld"
              },
              {
                "guard": "{{prefix}}hasKrankengeld",
                "target": "{{prefix}}krankengeld"
              },
              {
                "guard": "{{prefix}}hasElterngeld",
                "target": "{{prefix}}elterngeld"
              },
              {
                "guard": "{{prefix}}hasKindergeld",
                "target": "{{prefix}}kindergeld"
              },
              "#{{prefix}}weitere-einkuenfte"
            ],
            "BACK": [
              {
                "guard": "{{prefix}}receivesSupport",
                "target": "#{{prefix}}unterhalt"
              },
              "#{{prefix}}unterhalt-frage"
            ]
          }
        },
        "{{prefix}}wohngeld": {
          "on": {
            "SUBMIT": [
              {
                "guard": "{{prefix}}hasKrankengeld",
                "target": "{{prefix}}krankengeld"
              },
              {
                "guard": "{{prefix}}hasElterngeld",
                "target": "{{prefix}}elterngeld"
              },
              {
                "guard": "{{prefix}}hasKindergeld",
                "target": "#{{prefix}}leistungen.{{prefix}}kindergeld"
              },
              "#{{prefix}}weitere-einkuenfte"
            ],
            "BACK": "{{prefix}}frage"
          }
        },
        "{{prefix}}krankengeld": {
          "on": {
            "SUBMIT": [
              {
                "guard": "{{prefix}}hasElterngeld",
                "target": "{{prefix}}elterngeld"
              },
              {
                "guard": "{{prefix}}hasKindergeld",
                "target": "#{{prefix}}leistungen.{{prefix}}kindergeld"
              },
              "#{{prefix}}weitere-einkuenfte"
            ],
            "BACK": [
              {
                "guard": "{{prefix}}hasWohngeld",
                "target": "{{prefix}}wohngeld"
              },
              "{{prefix}}frage"
            ]
          }
        },
        "{{prefix}}elterngeld": {
          "on": {
            "SUBMIT": [
              {
                "guard": "{{prefix}}hasKindergeld",
                "target": "#{{prefix}}leistungen.{{prefix}}kindergeld"
              },
              "#{{prefix}}weitere-einkuenfte"
            ],
            "BACK": [
              {
                "guard": "{{prefix}}hasKrankengeld",
                "target": "{{prefix}}krankengeld"
              },
              {
                "guard": "{{prefix}}hasWohngeld",
                "target": "{{prefix}}wohngeld"
              },
              "{{prefix}}frage"
            ]
          }
        },
        "{{prefix}}kindergeld": {
          "on": {
            "SUBMIT": "#{{prefix}}weitere-einkuenfte",
            "BACK": [
              {
                "guard": "{{prefix}}hasElterngeld",
                "target": "{{prefix}}elterngeld"
              },
              {
                "guard": "{{prefix}}hasKrankengeld",
                "target": "{{prefix}}krankengeld"
              },
              {
                "guard": "{{prefix}}hasWohngeld",
                "target": "{{prefix}}wohngeld"
              },
              "{{prefix}}frage"
            ]
          }
        }
      }
    },
    "{{prefix}}weitere-einkuenfte": {
      "id": "{{prefix}}weitere-einkuenfte",
      "initial": "{{prefix}}frage",
      "states": {
        "{{prefix}}frage": {
          "on": {
            "SUBMIT": [
              {
                "guard": "{{prefix}}hasFurtherIncome",
                "target": "{{prefix}}uebersicht"
              },
              "#{{nextStep}}"
            ],
            "BACK": [
              {
                "guard": "{{prefix}}hasKindergeld",
                "target": "#{{prefix}}einkuenfte.{{prefix}}leistungen.{{prefix}}kindergeld"
              },
              {
                "guard": "{{prefix}}hasElterngeld",
                "target": "#{{prefix}}einkuenfte.{{prefix}}leistungen.{{prefix}}elterngeld"
              },
              {
                "guard": "{{prefix}}hasKrankengeld",
                "target": "#{{prefix}}einkuenfte.{{prefix}}leistungen.{{prefix}}krankengeld"
              },
              {
                "guard": "{{prefix}}hasWohngeld",
                "target": "#{{prefix}}einkuenfte.{{prefix}}leistungen.{{prefix}}wohngeld"
              },
              "#{{prefix}}einkuenfte.{{prefix}}leistungen"
            ]
          }
        },
        "{{prefix}}uebersicht": {
          "on": {
            "SUBMIT": [
              {
                "guard": "{{prefix}}hasFurtherIncomeAndEmptyArray",
                "target": "{{prefix}}warnung"
              },
              "#{{nextStep}}"
            ],
            "BACK": "{{prefix}}frage",
            "add-weitereEinkuenfte": {
              "guard": "{{prefix}}isValidEinkuenfteArrayIndex",
              "target": "{{prefix}}einkunft"
            }
          }
        },
        "{{prefix}}warnung": {
          "on": {
            "BACK": "{{prefix}}uebersicht",
            "SUBMIT": "#{{nextStep}}"
          }
        },
        "{{prefix}}einkunft": {
          "initial": "{{prefix}}daten",
          "states": {
            "{{prefix}}daten": {
              "on": {
                "BACK": "#{{prefix}}weitere-einkuenfte.{{prefix}}uebersicht",
                "SUBMIT": "#{{prefix}}weitere-einkuenfte.{{prefix}}uebersicht"
              }
            }
          }
        }
      }
    }
  }
}
