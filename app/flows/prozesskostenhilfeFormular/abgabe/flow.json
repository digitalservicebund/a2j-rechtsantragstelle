{
  "initial": "ueberpruefung",
  "id": "abgabe",
  "states": {
    "ueberpruefung": {
      "on": {
        "BACK": [
          {
            "guard": "hasAusgabenYes",
            "target": "#ausgaben-zusammenfassung"
          },
          "#ausgaben"
        ]
      },
      "always": { "guard": "readyForAbgabe", "target": "art" }
    },
    "art": {
      "on": {
        "SUBMIT": [
          { "target": "online", "guard": "abgabeOnline" },
          { "target": "ausdrucken", "guard": "abgabeAusdrucken" }
        ],
        "BACK": [
          {
            "guard": "hasGrundsicherungOrAsylbewerberleistungen",
            "target": "#finanzielle-angaben.einkuenfte"
          },
          {
            "guard": "hasAusgabenYes",
            "target": "#ausgaben-zusammenfassung"
          },
          "#ausgaben"
        ]
      }
    },
    "ausdrucken": { "on": { "BACK": { "target": "art" } } },
    "online": { "on": { "BACK": { "target": "art" } } }
  }
}
