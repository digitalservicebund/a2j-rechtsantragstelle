{
  "id": "forderung",
  "initial": "forderung",
  "states": {
    "forderung": {
      "on": {
        "SUBMIT": "teilentschaedigung",
        "BACK": "#persoenliche-daten.weitere-personen.uebersicht"
      }
    },
    "teilentschaedigung": {
      "on": { "SUBMIT": "nebenforderungen", "BACK": "forderung" }
