{
  "id": "forderung",
  "initial": "forderung",
  "states": {
    "forderung": {
      "on": {
        "SUBMIT": "teilentschaedigung",
        "BACK": [
          {
            "guard": "isProzessbevollmaechtigteNo",
            "target": "#persoenliche-daten.person.prozessbevollmaechtigte-angeben"
          },
          "#persoenliche-daten.person.prozessbevollmaechtigte-daten"
        ]
      }
    },
    "teilentschaedigung": {
      "on": { "SUBMIT": "nebenforderungen", "BACK": "forderung" }
    },
    "nebenforderungen": {
      "on": {
        "SUBMIT": "#versand.frist",
        "BACK": "teilentschaedigung"
      }
    }
  }
}
