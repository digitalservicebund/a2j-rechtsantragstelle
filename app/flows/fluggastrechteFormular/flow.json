{
  "id": "/fluggastrechte/formular",
  "initial": "intro",
  "states": {
    "intro": {
      "id": "intro",
      "initial": "start",
      "states": {
        "start": {
          "on": {
            "SUBMIT": "dokumente"
          }
        },
        "dokumente": {
          "on": {
            "SUBMIT": "daten-uebernahme",
            "BACK": "start"
          }
        },
        "daten-uebernahme": {
          "id": "daten-uebernahme",
          "on": {
            "SUBMIT": "#flugdaten.geplanter-flug",
            "BACK": "dokumente"
          }
        }
      }
    },
    "flugdaten": {},
    "persoenliche-daten": {},
    "forderung": {
      "id": "forderung",
      "initial": "forderung",
      "states": {
        "forderung": {
          "on": {
            "SUBMIT": "teilentschaedigung",
            "BACK": "#persoenliche-daten.bevollmaechtigte-person"
          }
        },
        "teilentschaedigung": {
          "on": { "SUBMIT": "nebenforderungen", "BACK": "forderung" }
        },
        "nebenforderungen": {
          "on": {
            "SUBMIT": "#versand.frist",
            "BACK": "teilentschaedigung"
          }
        }
      }
    },
    "versand": {
      "id": "versand",
      "initial": "frist",
      "states": {
        "frist": {
          "on": {
            "SUBMIT": "versaeumnisurteil",
            "BACK": "#forderung.nebenforderungen"
          }
        },
        "versaeumnisurteil": {
          "on": { "SUBMIT": "anmerkung", "BACK": "frist" }
        },
        "anmerkung": {
          "on": { "SUBMIT": "ueberpruefung", "BACK": "versaeumnisurteil" }
        },
        "ueberpruefung": {
          "on": { "SUBMIT": "aenderungMitteilung", "BACK": "anmerkung" }
        },
        "aenderungMitteilung": {
          "on": { "SUBMIT": "einverstaendnis", "BACK": "ueberpruefung" }
        },
        "einverstaendnis": {
          "on": { "SUBMIT": "klageVersenden", "BACK": "aenderungMitteilung" }
        },
        "klageVersenden": {
          "on": { "BACK": "einverstaendnis" }
        }
      }
    }
  }
}
