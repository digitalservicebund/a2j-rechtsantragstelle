{
  "initial": "start",
  "id": "anwaltlicheVertretung",
  "states": {
    "start": {
      "on": {
        "SUBMIT": [
          {
            "guard": "anwaltskanzleiYes",
            "target": "beratungStattgefunden"
          },
          {
            "target": "#rechtsproblem.start"
          }
        ],
        "BACK": "#grundvoraussetzungen.eigeninitiativeGrundvorraussetzung"
      }
    },
    "beratungStattgefunden": {
      "on": {
        "SUBMIT": [
          {
            "guard": "beratungStattgefundenYes",
            "target": "beratungStattgefundenDatum"
          },
          {
            "target": "#rechtsproblem.start"
          }
        ],
        "BACK": "start"
      }
    },
    "beratungStattgefundenDatum": {
      "on": {
        "SUBMIT": [
          {
            "guard": "beratungStattgefundenDatumLaterThanFourWeeks",
            "target": "anwalt-ende"
          },
          {
            "target": "frist-hinweis"
          }
        ],
        "BACK": "beratungStattgefunden"
      }
    },
    "frist-hinweis": {
      "on": {
        "SUBMIT": "anwaltKontaktdaten",
        "BACK": "beratungStattgefundenDatum"
      }
    },
    "anwaltKontaktdaten": {
      "on": {
        "SUBMIT": "#rechtsproblem.start",
        "BACK": "frist-hinweis"
      }
    },
    "anwalt-ende": {
      "on": { "BACK": "beratungStattgefundenDatum" }
    }
  }
}
