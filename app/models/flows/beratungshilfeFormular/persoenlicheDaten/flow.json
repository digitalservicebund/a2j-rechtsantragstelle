{
  "id": "persoenlicheDaten",
  "initial": "start",
  "states": {
    "start": {
      "on": {
        "SUBMIT": "name",
        "BACK": [
          {
            "guard": "staatlicheLeistungenIsBuergergeldAndBesitzDone",
            "target": "#finanzielleAngaben.eigentum-zusammenfassung.zusammenfassung"
          },
          {
            "guard": "staatlicheLeistungenIsBuergergeld",
            "target": "#finanzielleAngaben.eigentum.gesamtwert"
          },
          {
            "guard": "hasAusgabenYes",
            "target": "#ausgaben.uebersicht"
          },
          {
            "guard": "hasNoStaatlicheLeistungen",
            "target": "#ausgaben.ausgaben-frage"
          },
          "#finanzielleAngaben.einkommen.staatliche-leistungen"
        ]
      }
    },
    "name": { "on": { "BACK": "start", "SUBMIT": "geburtsdatum" } },
    "geburtsdatum": { "on": { "BACK": "name", "SUBMIT": "adresse" } },
    "adresse": { "on": { "BACK": "geburtsdatum", "SUBMIT": "telefonnummer" } },
    "telefonnummer": {
      "on": {
        "BACK": "adresse",
        "SUBMIT": { "target": "#abgabe", "guard": "readyForAbgabe" }
      }
    }
  }
}
