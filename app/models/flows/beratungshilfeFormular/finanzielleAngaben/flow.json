{
  "predictableActionArguments": true,
  "initial": "einkommen",
  "id": "finanzielleAngaben",
  "states": {
    "einkommen": {
      "id": "einkommen",
      "initial": "staatliche-leistungen",
      "states": {
        "staatliche-leistungen": {
          "on": {
            "BACK": "#finanzielleAngaben",
            "SUBMIT": [
              {
                "cond": "staatlicheLeistungenIsBuergergeld",
                "target": "#besitz.bankkonto-frage"
              },
              {
                "cond": "staatlicheLeistungenIsAsylbewerberleistungen",
                "target": "#finanzielleAngaben.danke"
              },
              {
                "cond": "staatlicheLeistungenIsGrundsicherung",
                "target": "#finanzielleAngaben.danke"
              },
              "erwerbstaetig"
            ]
          }
        },
        "erwerbstaetig": {
          "on": {
            "BACK": "staatliche-leistungen",
            "SUBMIT": [
              {
                "cond": "erwerbstaetigYes",
                "target": "art"
              },
              "situation"
            ]
          }
        },
        "art": {
          "on": {
            "BACK": "erwerbstaetig",
            "SUBMIT": "situation"
          }
        },
        "situation": {
          "on": {
            "BACK": [
              {
                "cond": "erwerbstaetigYes",
                "target": "art"
              },
              "erwerbstaetig"
            ],
            "SUBMIT": "weiteres-einkommen"
          }
        },
        "weiteres-einkommen": {
          "on": {
            "SUBMIT": "einkommen",
            "BACK": "situation"
          }
        },
        "einkommen": {
          "on": {
            "BACK": "weiteres-einkommen",
            "SUBMIT": "#partner.partnerschaft"
          }
        }
      }
    },
    "partner": {
      "id": "partner",
      "initial": "partnerschaft",
      "states": {
        "partnerschaft": {
          "on": {
            "BACK": "#einkommen.einkommen",
            "SUBMIT": [
              {
                "cond": "partnerschaftNo",
                "target": "#besitz.bankkonto-frage"
              },
              {
                "cond": "partnerschaftYes",
                "target": "zusammenleben"
              }
            ]
          }
        },
        "zusammenleben": {
          "on": {
            "BACK": "partnerschaft",
            "SUBMIT": [
              {
                "cond": "zusammenlebenYes",
                "target": "partner-einkommen"
              },
              {
                "cond": "zusammenlebenNo",
                "target": "unterhalt"
              }
            ]
          }
        },
        "unterhalt": {
          "on": {
            "BACK": "zusammenleben",
            "SUBMIT": [
              {
                "cond": "unterhaltYes",
                "target": "unterhalts-summe"
              },
              {
                "cond": "unterhaltNo",
                "target": "keine-rolle"
              }
            ]
          }
        },
        "keine-rolle": {
          "on": {
            "BACK": "unterhalt",
            "SUBMIT": "#besitz.bankkonto-frage"
          }
        },
        "unterhalts-summe": {
          "on": {
            "BACK": "unterhalt",
            "SUBMIT": "partner-name"
          }
        },
        "partner-name": {
          "on": {
            "BACK": "unterhalts-summe",
            "SUBMIT": "#besitz.bankkonto-frage"
          }
        },
        "partner-einkommen": {
          "on": {
            "BACK": "zusammenleben",
            "SUBMIT": [
              {
                "cond": "partnerEinkommenYes",
                "target": "partner-einkommen-summe"
              },
              {
                "cond": "partnerEinkommenNo",
                "target": "#besitz.bankkonto-frage"
              }
            ]
          }
        },
        "partner-einkommen-summe": {
          "on": {
            "BACK": "partner-einkommen",
            "SUBMIT": "#besitz.bankkonto-frage"
          }
        }
      }
    },
    "besitz": {
      "id": "besitz",
      "initial": "bankkonto-frage",
      "states": {
        "bankkonto-frage": {
          "id": "bankkonto-frage",
          "on": {
            "BACK": [
              {
                "cond": "hasStaatlicheLeistungen",
                "target": "#einkommen.staatliche-leistungen"
              },
              {
                "cond": "partnerschaftNo",
                "target": "#partner.partnerschaft"
              },
              {
                "cond": "isPartnerschaftZusammenlebenEinkommenNo",
                "target": "#partner.partner-einkommen"
              },
              {
                "cond": "isPartnerschaftZusammenlebenEinkommenYes",
                "target": "#partner.partner-einkommen-summe"
              },
              {
                "cond": "unterhaltNo",
                "target": "#partner.keine-rolle"
              },
              {
                "cond": "unterhaltYes",
                "target": "#partner.partner-name"
              },
              "#partner.partnerschaft"
            ],
            "SUBMIT": [
              {
                "cond": "hasBankkontoYes",
                "target": "bankkonten"
              },
              "kraftfahrzeug-frage"
            ]
          }
        },
        "bankkonten": {
          "id": "bankkonten",
          "on": {
            "SUBMIT": "weiteres-bankkonto",
            "BACK": "bankkonto"
          }
        },
        "weiteres-bankkonto": {
          "id": "weiteres-bankkonto",
          "on": {
            "BACK": "bankkonten",
            "SUBMIT": [
              { "target": "bankkonten", "cond": "hasAdditionalBankkontoYes" },
              "kraftfahrzeug-frage"
            ]
          }
        },
        "kraftfahrzeug-frage": {
          "id": "kraftfahrzeug-frage",
          "on": {
            "SUBMIT": [
              {
                "cond": "hasKraftfahrzeugYes",
                "target": "kraftfahrzeuge"
              },
              "zusammenfassung"
            ],
            "BACK": [
              {
                "cond": "hasBankkontoYes",
                "target": "weiteres-bankkonto"
              },
              "bankkonto"
            ]
          }
        },
        "kraftfahrzeuge": {
          "id": "kraftfahrzeuge",
          "on": {
            "SUBMIT": "weiteres-kraftfahrzeug",
            "BACK": "kraftfahrzeug-frage"
          }
        },
        "weiteres-kraftfahrzeug": {
          "id": "weiteres-kraftfahrzeug",
          "on": {
            "SUBMIT": [
              {
                "target": "kraftfahrzeuge",
                "cond": "hasAdditionalKraftfahrzeugYes"
              },
              "zusammenfassung"
            ],
            "BACK": "kraftfahrzeuge"
          }
        },
        "zusammenfassung": {
          "id": "zusammenfassung",
          "on": {
            "BACK": "weiteres-bankkonto",
            "SUBMIT": ["#danke"]
          }
        }
      }
    },
    "danke": {
      "id": "danke",
      "on": {
        "BACK": [
          {
            "cond": "staatlicheLeistungenIsAsylbewerberleistungen",
            "target": "#einkommen.staatliche-leistungen"
          },
          {
            "cond": "staatlicheLeistungenIsGrundsicherung",
            "target": "#einkommen.staatliche-leistungen"
          },
          {
            "cond": "staatlicheLeistungenIsBuergergeld",
            "target": "#besitz.weiteres-bankkonto"
          }
        ]
      }
    }
  }
}
