{
  "initial": "einkommen",
  "id": "finanzielleAngaben",
  "states": {
    "einkommen": {
      "id": "einkommen",
      "initial": "staatliche-leistungen",
      "states": {
        "staatliche-leistungen": {
          "on": {
            "SUBMIT": [
              {
                "guard": "staatlicheLeistungenIsBuergergeld",
                "target": "#besitz.bankkonten-frage"
              },
              {
                "guard": "staatlicheLeistungenIsAsylbewerberleistungen",
                "target": "#finanzielleAngaben.danke"
              },
              {
                "guard": "staatlicheLeistungenIsGrundsicherung",
                "target": "#finanzielleAngaben.danke"
              },
              "erwerbstaetig"
            ]
          }
        },
        "erwerbstaetig": {
          "on": {
            "BACK": "staatliche-leistungen",
            "SUBMIT": [
              {
                "guard": "erwerbstaetigYes",
                "target": "art"
              },
              "situation"
            ]
          }
        },
        "art": {
          "on": {
            "BACK": "erwerbstaetig",
            "SUBMIT": "situation"
          }
        },
        "situation": {
          "on": {
            "BACK": [
              {
                "guard": "erwerbstaetigYes",
                "target": "art"
              },
              "erwerbstaetig"
            ],
            "SUBMIT": "weiteres-einkommen"
          }
        },
        "weiteres-einkommen": {
          "on": {
            "SUBMIT": "einkommen",
            "BACK": "situation"
          }
        },
        "einkommen": {
          "on": {
            "BACK": "weiteres-einkommen",
            "SUBMIT": "#partner.partnerschaft"
          }
        }
      }
    },
    "partner": {
      "id": "partner",
      "initial": "partnerschaft",
      "states": {
        "partnerschaft": {
          "on": {
            "BACK": "#einkommen.einkommen",
            "SUBMIT": [
              {
                "guard": "partnerschaftNo",
                "target": "#kinder.kinder-frage"
              },
              {
                "guard": "partnerschaftYes",
                "target": "zusammenleben"
              }
            ]
          }
        },
        "zusammenleben": {
          "on": {
            "BACK": "partnerschaft",
            "SUBMIT": [
              {
                "guard": "zusammenlebenYes",
                "target": "partner-einkommen"
              },
              {
                "guard": "zusammenlebenNo",
                "target": "unterhalt"
              }
            ]
          }
        },
        "unterhalt": {
          "on": {
            "BACK": "zusammenleben",
            "SUBMIT": [
              {
                "guard": "unterhaltYes",
                "target": "unterhalts-summe"
              },
              {
                "guard": "unterhaltNo",
                "target": "keine-rolle"
              }
            ]
          }
        },
        "keine-rolle": {
          "on": {
            "BACK": "unterhalt",
            "SUBMIT": "#kinder.kinder-frage"
          }
        },
        "unterhalts-summe": {
          "on": {
            "BACK": "unterhalt",
            "SUBMIT": "partner-name"
          }
        },
        "partner-name": {
          "on": {
            "BACK": "unterhalts-summe",
            "SUBMIT": "#kinder.kinder-frage"
          }
        },
        "partner-einkommen": {
          "on": {
            "BACK": "zusammenleben",
            "SUBMIT": [
              {
                "guard": "partnerEinkommenYes",
                "target": "partner-einkommen-summe"
              },
              {
                "guard": "partnerEinkommenNo",
                "target": "#kinder.kinder-frage"
              }
            ]
          }
        },
        "partner-einkommen-summe": {
          "on": {
            "BACK": "partner-einkommen",
            "SUBMIT": "#kinder.kinder-frage"
          }
        }
      }
    },
    "kinder": {
      "id": "kinder",
      "initial": "kinder-frage",
      "states": {
        "kinder-frage": {
          "on": {
            "BACK": "#partner.partner-einkommen-summe",
            "SUBMIT": [
              {
                "guard": "hasKinderYes",
                "target": "zusammenfassung"
              },
              "#besitz.bankkonten-frage"
            ]
          }
        },
        "zusammenfassung": {
          "on": {
            "BACK": "kinder-frage",
            "SUBMIT": "#besitz",
            "ADDITEM": {
              "target": ["kinder", "haustiere"]
            }
          }
        },
        "kinder": {
          "id": "kinderCategory",
          "initial": "name",
          "states": {
            "name": {
              "on": {
                "BACK": "#kinder.zusammenfassung",
                "SUBMIT": "wohnort"
              }
            },
            "wohnort": {
              "on": {
                "BACK": "name",
                "SUBMIT": [
                  {
                    "guard": "kindWohnortBeiAntragstellerYes",
                    "target": "kind-eigene-einnahmen-frage"
                  },
                  "kind-unterhalt-frage"
                ]
              }
            },
            "kind-eigene-einnahmen-frage": {
              "on": {
                "BACK": "wohnort",
                "SUBMIT": [
                  {
                    "guard": "kindEigeneEinnahmenYes",
                    "target": "kind-eigene-einnahmen"
                  },
                  "#kinder.zusammenfassung"
                ]
              }
            },
            "kind-eigene-einnahmen": {
              "on": {
                "BACK": "kind-eigene-einnahmen-frage",
                "SUBMIT": "#kinder.zusammenfassung"
              }
            },
            "kind-unterhalt-frage": {
              "on": {
                "BACK": "wohnort",
                "SUBMIT": [
                  {
                    "guard": "kindUnterhaltYes",
                    "target": "kind-unterhalt"
                  },
                  "kind-unterhalt-ende"
                ]
              }
            },
            "kind-unterhalt": {
              "on": {
                "BACK": "kind-unterhalt-frage",
                "SUBMIT": "#kinder.zusammenfassung"
              }
            },
            "kind-unterhalt-ende": {
              "on": {
                "BACK": "kind-unterhalt-frage",
                "SUBMIT": "#kinder.zusammenfassung"
              }
            }
          }
        },
        "haustiere": {
          "id": "haustiere",
          "initial": "name",
          "states": {
            "name": {
              "on": {}
            }
          }
        }
      }
    },
    "besitz": {
      "id": "besitz",
      "initial": "zusammenfassung",
      "states": {
        "zusammenfassung": {
          "id": "zusammenfassung",
          "on": {
            "SUBMIT": "#danke",
            "ADDITEM": {
              "target": [
                "bankkonten",
                "wertsachen",
                "grundeigentum",
                "geldanlagen",
                "kraftfahrzeuge"
              ]
            }
          }
        },
        "bankkonten-frage": {
          "id": "bankkonto-frage",
          "on": {
            "BACK": [
              {
                "guard": "hasStaatlicheLeistungen",
                "target": "#einkommen.staatliche-leistungen"
              },
              {
                "guard": "partnerschaftNo",
                "target": "#partner.partnerschaft"
              },
              {
                "guard": "isPartnerschaftZusammenlebenEinkommenNo",
                "target": "#partner.partner-einkommen"
              },
              {
                "guard": "isPartnerschaftZusammenlebenEinkommenYes",
                "target": "#partner.partner-einkommen-summe"
              },
              {
                "guard": "unterhaltNo",
                "target": "#partner.keine-rolle"
              },
              {
                "guard": "unterhaltYes",
                "target": "#partner.partner-name"
              },
              "#partner.partnerschaft"
            ],
            "SUBMIT": [
              {
                "guard": "hasBankkontoYes",
                "target": "bankkonten"
              },
              "kraftfahrzeuge-frage"
            ]
          }
        },
        "bankkonten": {
          "id": "bankkonten",
          "initial": "daten",
          "states": {
            "daten": {
              "on": {
                "SUBMIT": "#besitz.weiteres-bankkonto",
                "BACK": "#besitz.bankkonten-frage"
              }
            }
          }
        },
        "kraftfahrzeuge": {
          "id": "kraftfahrzeuge",
          "initial": "daten",
          "states": {
            "daten": {
              "on": {
                "SUBMIT": "#besitz.weiteres-kraftfahrzeug",
                "BACK": "#besitz.kraftfahrzeuge-frage"
              }
            }
          }
        },
        "geldanlagen": {
          "id": "geldanlagen",
          "initial": "daten",
          "states": {
            "daten": {
              "on": {
                "SUBMIT": "#besitz.weitere-geldanlage",
                "BACK": "#besitz.geldanlagen-frage"
              }
            }
          }
        },
        "grundeigentum": {
          "id": "grundeigentum",
          "initial": "daten",
          "states": {
            "daten": {
              "on": {
                "SUBMIT": "#besitz.weiteres-grundeigentum",
                "BACK": "#besitz.grundeigentum-frage"
              }
            }
          }
        },
        "wertsachen": {
          "id": "wertsachen",
          "initial": "daten",
          "states": {
            "daten": {
              "on": {
                "SUBMIT": "#besitz.weitere-wertsache",
                "BACK": "#besitz.wertsachen-frage"
              }
            }
          }
        },
        "weiteres-bankkonto": {
          "id": "weiteres-bankkonto",
          "on": {
            "BACK": "bankkonten-frage",
            "SUBMIT": [
              {
                "target": "bankkonten",
                "guard": "hasAdditionalBankkontoYes"
              },
              "kraftfahrzeuge-frage"
            ]
          }
        },
        "kraftfahrzeuge-frage": {
          "id": "kraftfahrzeuge-frage",
          "on": {
            "SUBMIT": [
              {
                "guard": "hasKraftfahrzeugYes",
                "target": "kraftfahrzeuge"
              },
              "geldanlagen-frage"
            ],
            "BACK": [
              {
                "guard": "hasBankkontoYes",
                "target": "weiteres-bankkonto"
              },
              "bankkonten-frage"
            ]
          }
        },
        "weiteres-kraftfahrzeug": {
          "id": "weiteres-kraftfahrzeug",
          "on": {
            "SUBMIT": [
              {
                "target": "kraftfahrzeuge",
                "guard": "hasAdditionalKraftfahrzeugYes"
              },
              "geldanlagen-frage"
            ],
            "BACK": "kraftfahrzeuge-frage"
          }
        },
        "geldanlagen-frage": {
          "id": "geldanlagen-frage",
          "on": {
            "SUBMIT": [
              {
                "guard": "hasGeldanlageYes",
                "target": "geldanlagen"
              },
              "grundeigentum-frage"
            ],
            "BACK": [
              {
                "guard": "hasKraftfahrzeugYes",
                "target": "weiteres-kraftfahrzeug"
              },
              "kraftfahrzeuge-frage"
            ]
          }
        },
        "weitere-geldanlage": {
          "id": "weitere-geldanlage",
          "on": {
            "SUBMIT": [
              {
                "target": "geldanlagen",
                "guard": "hasAdditionalGeldanlageYes"
              },
              "grundeigentum-frage"
            ],
            "BACK": "geldanlagen-frage"
          }
        },
        "grundeigentum-frage": {
          "id": "grundeigentum-frage",
          "on": {
            "SUBMIT": [
              {
                "guard": "hasGrundeigentumYes",
                "target": "grundeigentum"
              },
              "wertsachen-frage"
            ],
            "BACK": "weitere-geldanlage"
          }
        },
        "weiteres-grundeigentum": {
          "id": "weiteres-grundeigentum",
          "on": {
            "SUBMIT": [
              {
                "target": "grundeigentum",
                "guard": "hasAdditionalGrundeigentumYes"
              },
              "wertsachen-frage"
            ],
            "BACK": "grundeigentum-frage"
          }
        },
        "wertsachen-frage": {
          "id": "wertsachen-frage",
          "on": {
            "SUBMIT": [
              {
                "guard": "hasWertsacheYes",
                "target": "wertsachen"
              },
              "zusammenfassung"
            ],
            "BACK": "weiteres-grundeigentum"
          }
        },
        "weitere-wertsache": {
          "id": "weitere-wertsache",
          "on": {
            "SUBMIT": [
              {
                "target": "wertsachen",
                "guard": "hasAdditionalWertsacheYes"
              },
              "zusammenfassung"
            ],
            "BACK": "wertsachen-frage"
          }
        }
      }
    },
    "danke": {
      "id": "danke",
      "on": {
        "BACK": [
          {
            "guard": "staatlicheLeistungenIsAsylbewerberleistungen",
            "target": "#einkommen.staatliche-leistungen"
          },
          {
            "guard": "staatlicheLeistungenIsGrundsicherung",
            "target": "#einkommen.staatliche-leistungen"
          },
          "#besitz.zusammenfassung"
        ]
      }
    }
  }
}
