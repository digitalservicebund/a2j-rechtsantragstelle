{
  "predictableActionArguments": true,
  "initial": "start",
  "id": "finanzielleAngaben",
  "states": {
    "start": {
      "on": { "SUBMIT": "#beruf.staatliche-leistungen" }
    },
    "beruf": {
      "id": "beruf",
      "states": {
        "staatliche-leistungen": {
          "on": {
            "BACK": "#finanzielleAngaben.start",
            "SUBMIT": [
              {
                "cond": "staatlicheLeistungenIsBuergergeld",
                "target": "#besitz.besitz"
              },
              {
                "cond": "staatlicheLeistungenIsAsylbewerberleistungen",
                "target": "#finanzielleAngaben.danke"
              },
              {
                "cond": "staatlicheLeistungenIsGrundsicherung",
                "target": "#finanzielleAngaben.danke"
              },
              "erwerbstaetig"
            ]
          }
        },
        "erwerbstaetig": {
          "on": {
            "BACK": "staatliche-leistungen",
            "SUBMIT": [
              {
                "cond": "erwerbstaetigYes",
                "target": "art"
              },
              "situation"
            ]
          }
        },
        "art": {
          "on": {
            "BACK": "erwerbstaetig",
            "SUBMIT": "situation"
          }
        },
        "situation": {
          "on": {
            "BACK": [
              {
                "cond": "erwerbstaetigYes",
                "target": "art"
              },
              "erwerbstaetig"
            ],
            "SUBMIT": "weiteres-einkommen"
          }
        },
        "weiteres-einkommen": {
          "on": {
            "SUBMIT": "#einkommen.einkommen",
            "BACK": "situation"
          }
        }
      }
    },
    "einkommen": {
      "id": "einkommen",
      "initial": "einkommen",
      "states": {
        "einkommen": {
          "on": {
            "BACK": "#beruf.weiteres-einkommen",
            "SUBMIT": "#partner.partnerschaft"
          }
        }
      }
    },
    "partner": {
      "id": "partner",
      "initial": "partnerschaft",
      "states": {
        "partnerschaft": {
          "on": {
            "BACK": "#einkommen.einkommen",
            "SUBMIT": [
              {
                "cond": "partnerschaftNo",
                "target": "#besitz.besitz"
              },
              {
                "cond": "partnerschaftYes",
                "target": "zusammenleben"
              }
            ]
          }
        },
        "zusammenleben": {
          "on": {
            "BACK": "partnerschaft",
            "SUBMIT": [
              {
                "cond": "zusammenlebenYes",
                "target": "partner-einkommen"
              },
              {
                "cond": "zusammenlebenNo",
                "target": "unterhalt"
              }
            ]
          }
        },
        "unterhalt": {
          "on": {
            "BACK": "zusammenleben",
            "SUBMIT": [
              {
                "cond": "unterhaltYes",
                "target": "unterhalts-summe"
              },
              {
                "cond": "unterhaltNo",
                "target": "keine-rolle"
              }
            ]
          }
        },
        "keine-rolle": {
          "on": {
            "BACK": "unterhalt",
            "SUBMIT": "#besitz.besitz"
          }
        },
        "unterhalts-summe": {
          "on": {
            "BACK": "unterhalt",
            "SUBMIT": "partner-name"
          }
        },
        "partner-name": {
          "on": {
            "BACK": "unterhalts-summe",
            "SUBMIT": "#besitz.besitz"
          }
        },
        "partner-einkommen": {
          "on": {
            "BACK": "zusammenleben",
            "SUBMIT": [
              {
                "cond": "partnerEinkommenYes",
                "target": "partner-einkommen-summe"
              },
              {
                "cond": "partnerEinkommenNo",
                "target": "#besitz.besitz"
              }
            ]
          }
        },
        "partner-einkommen-summe": {
          "on": {
            "BACK": "partner-einkommen",
            "SUBMIT": "#besitz.besitz"
          }
        }
      }
    },
    "besitz": {
      "id": "besitz",
      "states": {
        "besitz": {
          "on": {
            "BACK": [
              {
                "cond": "partnerschaftNo",
                "target": "#partner.partnerschaft"
              },
              {
                "cond": "isPartnerschaftZusammenlebenEinkommenNo",
                "target": "#partner.partner-einkommen"
              },
              {
                "cond": "isPartnerschaftZusammenlebenEinkommenYes",
                "target": "#partner.partner-einkommen-summe"
              },
              {
                "cond": "unterhaltNo",
                "target": "#partner.keine-rolle"
              },
              {
                "cond": "unterhaltYes",
                "target": "#partner.partner-name"
              },
              "#partner.partnerschaft"
            ],
            "SUBMIT": "#danke"
          }
        }
      }
    },
    "danke": {
      "id": "danke",
      "on": {
        "BACK": [
          {
            "cond": "staatlicheLeistungenIsAsylbewerberleistungen",
            "target": "#beruf.staatliche-leistungen"
          },
          {
            "cond": "staatlicheLeistungenIsGrundsicherung",
            "target": "#beruf.staatliche-leistungen"
          },
          {
            "cond": "staatlicheLeistungenIsBuergergeld",
            "target": "#besitz.besitz"
          }
        ]
      }
    }
  }
}
