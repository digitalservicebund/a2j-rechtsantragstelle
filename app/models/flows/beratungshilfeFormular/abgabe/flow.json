{
  "predictableActionArguments": true,
  "initial": "art",
  "id": "abgabe",
  "states": {
    "art": {
      "on": {
        "SUBMIT": [
          { "target": "online", "cond": "abgabeOnline" },
          { "target": "download", "cond": "abgabeDownload" }
        ]
      }
    },
    "download": {
      "meta": {
        "buttonNavigationProps": {
          "next": {
            "destination": "/beratungshilfe/antrag/pdf",
            "downloadFile": "Beratungshilfe Antrag.pdf"
          }
        }
      },
      "on": {
        "BACK": {
          "target": "art"
        }
      }
    },
    "online": {
      "meta": {
        "buttonNavigationProps": {
          "next": {
            "destination": "/beratungshilfe/antrag/pdf",
            "downloadFile": "Beratungshilfe Antrag.pdf"
          }
        }
      },
      "on": {
        "BACK": {
          "target": "art"
        }
      }
    }
  }
}
