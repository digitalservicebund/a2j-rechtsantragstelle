{
  "id": "/geld-einklagen/vorabcheck/",
  "initial": "start",
  "states": {
    "start": {
      "meta": { "progressPosition": 1 },
      "on": {
        "SUBMIT": "gerichtskostenvorschuss"
      }
    },
    "gerichtskostenvorschuss": {
      "meta": { "progressPosition": 2 },
      "on": {
        "SUBMIT": [
          {
            "target": "ergebnis/gerichtskostenvorschuss-abbruch",
            "cond": "gerichtskostenvorschussNo"
          },
          {
            "target": "ergebnis/gerichtskostenvorschuss-hinweis",
            "cond": "gerichtskostenvorschussNotPossible"
          },
          {
            "target": "forderung",
            "cond": "gerichtskostenvorschussYes"
          }
        ],
        "BACK": { "target": "start" }
      }
    },
    "ergebnis/gerichtskostenvorschuss-hinweis": {
      "meta": { "progressPosition": 3 },
      "on": {
        "SUBMIT": { "target": "forderung" },
        "BACK": { "target": "gerichtskostenvorschuss" }
      }
    },
    "ergebnis/gerichtskostenvorschuss-abbruch": {
      "meta": { "progressPosition": 3 },
      "on": {
        "BACK": { "target": "gerichtskostenvorschuss" }
      }
    },
    "forderung": {
      "meta": { "progressPosition": 18 },
      "on": {
        "SUBMIT": [
          {
            "target": "ergebnis/forderung-abbruch",
            "cond": "forderungIsNotLessOrEqual5000"
          },
          {
            "target": "bereich",
            "cond": "forderungIsLessOrEqual5000"
          }
        ],
        "BACK": [
          {
            "target": "ergebnis/gerichtskostenvorschuss-hinweis",
            "cond": "gerichtskostenvorschussNotPossible"
          },
          {
            "target": "gerichtskostenvorschuss",
            "cond": "gerichtskostenvorschussYes"
          }
        ]
      }
    },
    "ergebnis/forderung-abbruch": {
      "meta": { "progressPosition": 19 },
      "on": {
        "BACK": { "target": "forderung" }
      }
    },
    "bereich": {
      "meta": { "progressPosition": 20 },
      "on": {
        "SUBMIT": [
          {
            "target": "ergebnis/bereich-familie-abbruch",
            "cond": "bereichFamily"
          },
          {
            "target": "ergebnis/bereich-arbeit-abbruch",
            "cond": "bereichWork"
          },
          {
            "target": "flug",
            "cond": "bereichTravel"
          },
          {
            "target": "gegenseite",
            "cond": "bereichFilled"
          }
        ],
        "BACK": {
          "target": "forderung"
        }
      }
    },
    "ergebnis/bereich-familie-abbruch": {
      "meta": { "progressPosition": 21 },
      "on": {
        "SUBMIT": [],
        "BACK": { "target": "bereich" }
      }
    },
    "ergebnis/bereich-arbeit-abbruch": {
      "meta": { "progressPosition": 21 },
      "on": {
        "SUBMIT": [],
        "BACK": { "target": "bereich" }
      }
    },
    "flug": {
      "meta": { "progressPosition": 22 },
      "on": {
        "SUBMIT": [
          {
            "target": "ergebnis/flug-abbruch",
            "cond": "flugYes"
          },
          {
            "target": "gegenseite",
            "cond": "flugNo"
          }
        ],
        "BACK": {
          "target": "bereich"
        }
      }
    },
    "ergebnis/flug-abbruch": {
      "meta": { "progressPosition": 23 },
      "on": {
        "BACK": { "target": "flug" }
      }
    },

    "gegenseite": {
      "meta": { "progressPosition": 24 },
      "on": {
        "SUBMIT": [
          {
            "target": "ergebnis/gegenseite-mehrere-abbruch",
            "cond": "gegenseiteMultiple"
          },
          {
            "target": "ergebnis/gegenseite-staat-abbruch",
            "cond": "gegenseiteStaat"
          },
          {
            "target": "kontaktaufnahme",
            "cond": "gegenseiteUnternehmen"
          },
          {
            "target": "kontaktaufnahme",
            "cond": "gegenseitePrivatperson"
          }
        ],
        "BACK": [
          { "target": "flug", "cond": "bereichTravel" },
          { "target": "bereich", "cond": "bereichFilled" }
        ]
      }
    },
    "ergebnis/gegenseite-mehrere-abbruch": {
      "meta": { "progressPosition": 25 },
      "on": { "BACK": "gegenseite" }
    },
    "ergebnis/gegenseite-staat-abbruch": {
      "meta": { "progressPosition": 25 },
      "on": { "BACK": "gegenseite" }
    },

    "kontaktaufnahme": {
      "meta": { "progressPosition": 2 },
      "on": {
        "SUBMIT": [
          {
            "target": "ergebnis/kontaktaufnahme-hinweis",
            "cond": "kontaktaufnahmeNo"
          },
          {
            "target": "frist-abgelaufen",
            "cond": "kontaktaufnahmeYes"
          }
        ],
        "BACK": "gegenseite"
      }
    },
    "ergebnis/kontaktaufnahme-hinweis": {
      "meta": { "progressPosition": 3 },
      "on": {
        "SUBMIT": { "target": "frist-abgelaufen" },
        "BACK": { "target": "kontaktaufnahme" }
      }
    },
    "frist-abgelaufen": {
      "meta": { "progressPosition": 4 },
      "on": {
        "SUBMIT": [
          {
            "target": "privatperson",
            "cond": "fristAbgelaufenYes"
          },
          {
            "target": "ergebnis/frist-abgelaufen-hinweis",
            "cond": "fristAbgelaufenNo"
          },
          {
            "target": "ergebnis/frist-abgelaufen-hinweis",
            "cond": "fristAbgelaufenNotSet"
          }
        ],
        "BACK": [
          {
            "target": "ergebnis/kontaktaufnahme-hinweis",
            "cond": "kontaktaufnahmeNo"
          },
          {
            "target": "kontaktaufnahme",
            "cond": "kontaktaufnahmeYes"
          }
        ]
      }
    },
    "ergebnis/frist-abgelaufen-hinweis": {
      "meta": { "progressPosition": 5 },
      "on": {
        "SUBMIT": "privatperson",
        "BACK": "frist-abgelaufen"
      }
    },
    "privatperson": {
      "meta": { "progressPosition": 14 },
      "on": {
        "SUBMIT": [
          {
            "target": "bund-id-konto",
            "cond": "privatpersonEligible"
          },
          {
            "target": "ergebnis/privatperson-abbruch",
            "cond": "privatpersonNotEligible"
          }
        ],
        "BACK": [
          {
            "target": "frist-abgelaufen",
            "cond": "fristAbgelaufenYes"
          },
          {
            "target": "ergebnis/frist-abgelaufen-hinweis",
            "cond": "fristAbgelaufenNo"
          },
          {
            "target": "ergebnis/frist-abgelaufen-hinweis",
            "cond": "fristAbgelaufenNotSet"
          }
        ]
      }
    },
    "ergebnis/privatperson-abbruch": {
      "meta": { "progressPosition": 15 },
      "on": { "BACK": "privatperson" }
    },
    "bund-id-konto": {
      "meta": { "progressPosition": 16 },
      "on": {
        "SUBMIT": [
          {
            "target": "ergebnis/abschluss",
            "cond": "bundIdAccountYes"
          },
          {
            "target": "ergebnis/abschluss",
            "cond": "bundIdAccountWantTo"
          },
          {
            "target": "ergebnis/bund-id-konto-abbruch",
            "cond": "bundIdAccountNo"
          }
        ],
        "BACK": "privatperson"
      }
    },
    "ergebnis/bund-id-konto-abbruch": {
      "meta": { "progressPosition": 17 },
      "on": { "BACK": "bund-id-konto" }
    },

    "ergebnis/abschluss": {
      "meta": { "progressPosition": 26 },
      "on": { "BACK": "bund-id-konto" }
    }
  }
}
