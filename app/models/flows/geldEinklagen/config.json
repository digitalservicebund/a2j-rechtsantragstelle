{
  "id": "/geld-einklagen/vorabcheck/",
  "initial": "start",
  "states": {
    "start": {
      "meta": { "progressPosition": 1 },
      "on": {
        "SUBMIT": "gerichstkostenvorschuss"
      }
    },
    "gerichstkostenvorschuss": {
      "meta": { "progressPosition": 2 },
      "on": {
        "SUBMIT": [
          {
            "target": "ergebnis/gerichstkostenvorschuss-abbruch",
            "cond": "gerichstkostenvorschussNo"
          },
          {
            "target": "ergebnis/gerichstkostenvorschuss-hinweis",
            "cond": "gerichstkostenvorschussNotPossible"
          },
          {
            "target": "kontaktaufnahme",
            "cond": "gerichstkostenvorschussYes"
          }
        ],
        "BACK": { "target": "start" }
      }
    },
    "ergebnis/gerichstkostenvorschuss-hinweis": {
      "meta": { "progressPosition": 3 },
      "on": {
        "SUBMIT": { "target": "kontaktaufnahme" },
        "BACK": { "target": "gerichstkostenvorschuss" }
      }
    },
    "ergebnis/gerichstkostenvorschuss-abbruch": {
      "meta": { "progressPosition": 3 },
      "on": {
        "BACK": { "target": "gerichstkostenvorschuss" }
      }
    },
    "kontaktaufnahme": {
      "meta": { "progressPosition": 2 },
      "on": {
        "SUBMIT": [
          {
            "target": "ergebnis/kontaktaufnahme-hinweis",
            "cond": "kontaktaufnahmeNo"
          },
          {
            "target": "frist-abgelaufen",
            "cond": "kontaktaufnahmeYes"
          }
        ],
        "BACK": [
          {
            "target": "ergebnis/gerichstkostenvorschuss-hinweis",
            "cond": "gerichstkostenvorschussNotPossible"
          },
          {
            "target": "gerichstkostenvorschuss",
            "cond": "gerichstkostenvorschussYes"
          }
        ]
      }
    },
    "ergebnis/kontaktaufnahme-hinweis": {
      "meta": { "progressPosition": 3 },
      "on": {
        "SUBMIT": { "target": "frist-abgelaufen" },
        "BACK": { "target": "kontaktaufnahme" }
      }
    },
    "frist-abgelaufen": {
      "meta": { "progressPosition": 4 },
      "on": {
        "SUBMIT": [
          {
            "target": "privatperson",
            "cond": "fristAbgelaufenYes"
          },
          {
            "target": "ergebnis/frist-abgelaufen-hinweis",
            "cond": "fristAbgelaufenNo"
          },
          {
            "target": "ergebnis/frist-abgelaufen-hinweis",
            "cond": "fristAbgelaufenNotSet"
          }
        ],
        "BACK": [
          {
            "target": "ergebnis/kontaktaufnahme-hinweis",
            "cond": "kontaktaufnahmeNo"
          },
          {
            "target": "kontaktaufnahme",
            "cond": "kontaktaufnahmeYes"
          }
        ]
      }
    },
    "ergebnis/frist-abgelaufen-hinweis": {
      "meta": { "progressPosition": 5 },
      "on": {
        "SUBMIT": { "target": "privatperson" },
        "BACK": { "target": "frist-abgelaufen" }
      }
    },
    "privatperson": {
      "meta": { "progressPosition": 14 },
      "on": {
        "SUBMIT": [
          {
            "target": "bund-id-konto",
            "cond": "privatpersonYes"
          },
          {
            "target": "ergebnis/privatperson-abbruch",
            "cond": "notSinglePerson"
          }
        ],
        "BACK": [
          {
            "target": "frist-abgelaufen",
            "cond": "fristAbgelaufenYes"
          },
          {
            "target": "ergebnis/frist-abgelaufen-hinweis",
            "cond": "fristAbgelaufenNo"
          },
          {
            "target": "ergebnis/frist-abgelaufen-hinweis",
            "cond": "fristAbgelaufenNotSet"
          }
        ]
      }
    },
    "ergebnis/privatperson-abbruch": {
      "meta": { "progressPosition": 15 },
      "on": {
        "BACK": { "target": "privatperson" }
      }
    },
    "bund-id-konto": {
      "meta": { "progressPosition": 16 },
      "on": {
        "SUBMIT": [
          {
            "target": "forderung",
            "cond": "bundIdAccountYes"
          },
          {
            "target": "forderung",
            "cond": "bundIdAccountWantTo"
          },
          {
            "target": "ergebnis/bund-id-konto-abbruch",
            "cond": "bundIdAccountNo"
          }
        ],
        "BACK": { "target": "privatperson" }
      }
    },
    "ergebnis/bund-id-konto-abbruch": {
      "meta": { "progressPosition": 17 },
      "on": {
        "BACK": { "target": "bund-id-konto" }
      }
    },
    "forderung": {
      "meta": { "progressPosition": 18 },
      "on": {
        "SUBMIT": [
          {
            "target": "ergebnis/forderung-abbruch",
            "cond": "forderungIsNotLessOrEqual5000"
          },
          {
            "target": "bereich",
            "cond": "forderungIsLessOrEqual5000"
          }
        ],
        "BACK": { "target": "bund-id-konto" }
      }
    },
    "ergebnis/forderung-abbruch": {
      "meta": { "progressPosition": 19 },
      "on": {
        "BACK": { "target": "forderung" }
      }
    },
    "bereich": {
      "meta": { "progressPosition": 20 },
      "on": {
        "SUBMIT": [
          {
            "target": "ergebnis/bereich-familie-abbruch",
            "cond": "bereichFamily"
          },
          {
            "target": "ergebnis/bereich-arbeit-abbruch",
            "cond": "bereichWork"
          },
          {
            "target": "flug",
            "cond": "bereichTravel"
          },
          {
            "target": "gegenseite",
            "cond": "bereichFilled"
          }
        ],
        "BACK": {
          "target": "forderung"
        }
      }
    },
    "ergebnis/bereich-familie-abbruch": {
      "meta": { "progressPosition": 21 },
      "on": {
        "SUBMIT": [],
        "BACK": { "target": "bereich" }
      }
    },
    "ergebnis/bereich-arbeit-abbruch": {
      "meta": { "progressPosition": 21 },
      "on": {
        "SUBMIT": [],
        "BACK": { "target": "bereich" }
      }
    },
    "flug": {
      "meta": { "progressPosition": 22 },
      "on": {
        "SUBMIT": [
          {
            "target": "ergebnis/flug-abbruch",
            "cond": "flugYes"
          },
          {
            "target": "gegenseite",
            "cond": "flugNo"
          }
        ],
        "BACK": {
          "target": "bereich"
        }
      }
    },
    "ergebnis/flug-abbruch": {
      "meta": { "progressPosition": 23 },
      "on": {
        "BACK": { "target": "flug" }
      }
    },
    "gegenseite": {
      "meta": { "progressPosition": 24 },
      "on": {
        "SUBMIT": [
          {
            "target": "ergebnis/gegenseite-mehrere-abbruch",
            "cond": "gegenseiteMultiple"
          },
          {
            "target": "ergebnis/gegenseite-staat-abbruch",
            "cond": "gegenseiteStaat"
          },
          {
            "target": "gegenseite-person-deutschland",
            "cond": "gegenseitePrivatperson"
          },
          {
            "target": "gegenseite-unternehmen-deutschland",
            "cond": "gegenseiteUnternehmen"
          }
        ],
        "BACK": [
          { "target": "flug", "cond": "bereichTravel" },
          { "target": "bereich", "cond": "bereichFilled" }
        ]
      }
    },
    "ergebnis/gegenseite-mehrere-abbruch": {
      "meta": { "progressPosition": 25 },
      "on": {
        "BACK": { "target": "gegenseite" }
      }
    },
    "ergebnis/gegenseite-staat-abbruch": {
      "meta": { "progressPosition": 25 },
      "on": {
        "BACK": { "target": "gegenseite" }
      }
    },
    "gegenseite-person-deutschland": {
      "meta": { "progressPosition": 25 },
      "on": {
        "SUBMIT": [
          {
            "target": "ergebnis/gegenseite-person-deutschland-abbruch",
            "cond": "gegenseitePersonDeutschlandNo"
          },
          {
            "target": "ergebnis/abschluss",
            "cond": "gegenseitePersonDeutschlandYes"
          }
        ],
        "BACK": { "target": "gegenseite" }
      }
    },
    "ergebnis/gegenseite-person-deutschland-abbruch": {
      "meta": { "progressPosition": 26 },
      "on": {
        "BACK": { "target": "gegenseite-person-deutschland" }
      }
    },
    "gegenseite-unternehmen-deutschland": {
      "meta": { "progressPosition": 25 },
      "on": {
        "SUBMIT": [
          {
            "target": "ergebnis/gegenseite-unternehmen-deutschland-abbruch",
            "cond": "gegenseiteUnternehmenDeutschlandNo"
          },
          {
            "target": "ergebnis/abschluss",
            "cond": "gegenseiteUnternehmenDeutschlandYes"
          }
        ],
        "BACK": { "target": "gegenseite" }
      }
    },
    "ergebnis/gegenseite-unternehmen-deutschland-abbruch": {
      "meta": { "progressPosition": 26 },
      "on": {
        "BACK": { "target": "gegenseite-unternehmen-deutschland" }
      }
    },
    "ergebnis/abschluss": {
      "meta": { "progressPosition": 26 },
      "on": {
        "BACK": [
          {
            "target": "gegenseite-person-deutschland",
            "cond": "gegenseitePrivatperson"
          },
          {
            "target": "gegenseite-unternehmen-deutschland",
            "cond": "gegenseiteUnternehmen"
          }
        ]
      }
    }
  }
}
