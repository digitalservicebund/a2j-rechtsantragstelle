        "SUBMIT": [
          {
            "target": "verjaehrt",
            "cond": "fristAbgelaufenYes"
          },
          {
            "target": "fristAbgelaufen-hinweis",
            "cond": "fristAbgelaufenNo"
          },
          {
            "target": "fristAbgelaufen-hinweis",
