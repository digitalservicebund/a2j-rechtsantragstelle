{
  "id": "/geld-einklagen/vorabcheck/",
  "initial": "kontaktaufnahme",
  "states": {
    "kontaktaufnahme": {
      "meta": { "progressPosition": 1 },
      "on": {
        "SUBMIT": [
          {
            "target": "kontaktaufnahme-hinweis",
            "cond": "kontaktaufnahmeNo"
          },
          {
            "target": "fristAbgelaufen",
            "cond": "kontaktaufnahmeYes"
          }
        ]
      }
    },
    "kontaktaufnahme-hinweis": {
      "meta": { "progressPosition": 2 },
      "on": {
        "SUBMIT": { "target": "fristAbgelaufen" },
        "BACK": { "target": "kontaktaufnahme" }
      }
    },
    "fristAbgelaufen": {
      "meta": { "progressPosition": 3 },
      "on": {
        "SUBMIT": [
          {
            "target": "verjaehrt",
            "cond": "fristAbgelaufenYes"
          },
          {
            "target": "fristAbgelaufen-hinweis",
            "cond": "fristAbgelaufenNo"
          },
          {
            "target": "fristAbgelaufen-hinweis",
            "cond": "fristAbgelaufenNotSet"
          }
        ],
        "BACK": { "target": "kontaktaufnahme" }
      }
    },
    "fristAbgelaufen-hinweis": {
      "meta": { "progressPosition": 4 },
      "on": {
        "SUBMIT": { "target": "verjaehrt" },
        "BACK": { "target": "fristAbgelaufen" }
      }
    },
    "verjaehrt": {
      "meta": { "progressPosition": 5 },
      "on": {
        "SUBMIT": [
          {
            "target": "verjaehrt-hinweis",
            "cond": "verjaehrtYes"
          },
          {
            "target": "beweise",
            "cond": "verjaehrtNo"
          }
        ],
        "BACK": { "target": "fristAbgelaufen" }
      }
    },
    "verjaehrt-hinweis": {
      "meta": { "progressPosition": 6 },
      "on": {
        "SUBMIT": { "target": "beweise" },
        "BACK": { "target": "verjaehrt" }
      }
    },
    "beweise": {
      "meta": { "progressPosition": 7 },
      "on": {
        "SUBMIT": [
          {
            "target": "beweise-hinweis",
            "cond": "beweiseNo"
          },
          {
            "target": "gerichtsentscheidung",
            "cond": "beweiseYes"
          }
        ],
        "BACK": { "target": "verjaehrt" }
      }
