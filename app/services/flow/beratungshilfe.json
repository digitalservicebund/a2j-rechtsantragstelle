{
  "id": "Beratungshilfe",
  "initial": "rechtsschutzversicherung",
  "states": {
    "rechtsschutzversicherung": {
      "on": {
        "SUBMIT": [
          {
            "target": "wurdeVerklagt",
            "cond": "no"
          },
          {
            "target": "rechtsschutzversicherungError"
          }
        ]
      }
    },
    "rechtsschutzversicherungError": {
      "type": "final",
      "on": {
        "BACK": { "target": "rechtsschutzversicherung" }
      }
    },
    "wurdeVerklagt": {
      "on": {
        "SUBMIT": [
          {
            "target": "klageEingereicht",
            "cond": "no"
          },
          {
            "target": "wurdeVerklagtError"
          }
        ],
        "BACK": { "target": "rechtsschutzversicherung" }
      }
    },
    "wurdeVerklagtError": {
      "type": "final",
      "on": {
        "BACK": { "target": "wurdeVerklagt" }
      }
    },
    "klageEingereicht": {
      "on": {
        "SUBMIT": [
          {
            "target": "hamburgOderBremen",
            "cond": "no"
          },
          {
            "target": "klageEingereichtError"
          }
        ],
        "BACK": { "target": "wurdeVerklagt" }
      }
    },
    "klageEingereichtError": {
      "type": "final",
      "on": {
        "BACK": { "target": "klageEingereicht" }
      }
    },
    "hamburgOderBremen": {
      "on": {
        "SUBMIT": [
          {
            "target": "beratungshilfeBeantragt",
            "cond": "no"
          },
          {
            "target": "hamburgOderBremenError"
          }
        ],
        "BACK": { "target": "klageEingereicht" }
      }
    },
    "hamburgOderBremenError": {
      "type": "final",
      "on": {
        "BACK": { "target": "hamburgOderBremen" }
      }
    },
    "beratungshilfeBeantragt": {
      "on": {
        "SUBMIT": [
          {
            "target": "eigeninitiative",
            "cond": "no"
          },
          {
            "target": "beratungshilfeBeantragtError"
          }
        ],
        "BACK": { "target": "hamburgOderBremen" }
      }
    },
    "beratungshilfeBeantragtError": {
      "type": "final",
      "on": {
        "BACK": { "target": "beratungshilfeBeantragt" }
      }
    },
    "eigeninitiative": {
      "on": {
        "SUBMIT": [
          {
            "target": "kostenfreieBeratung",
            "cond": "yes"
          },
          {
            "target": "eigeninitiativeWarnung"
          }
        ],
        "BACK": { "target": "beratungshilfeBeantragt" }
      }
    },
    "eigeninitiativeWarnung": {
      "on": {
        "SUBMIT": {
          "target": "kostenfreieBeratung"
        },
        "BACK": { "target": "eigeninitiative" }
      }
    },
    "kostenfreieBeratung": {
      "on": {
        "SUBMIT": [
          {
            "target": "staatlicheLeistungen",
            "cond": "yes"
          },
          {
            "target": "kostenfreieBeratungWarnung"
          }
        ],
        "BACK": { "target": "eigeninitiative" }
      }
    },
    "kostenfreieBeratungWarnung": {
      "on": {
        "SUBMIT": {
          "target": "staatlicheLeistungen"
        },
        "BACK": { "target": "kostenfreieBeratung" }
      }
    },
    "staatlicheLeistungen": {
      "on": {
        "SUBMIT": [
          {
            "target": "staatlicheLeistungenAbschlussJa",
            "cond": "staatlicheLeistung"
          },
          {
            "target": "vermoegen"
          }
        ],
        "BACK": { "target": "kostenfreieBeratung" }
      }
    },
    "staatlicheLeistungenAbschlussJa": {
      "type": "final",
      "on": {
        "BACK": { "target": "staatlicheLeistungen" }
      }
    },
    "vermoegen": {
      "on": {
        "SUBMIT": [
          {
            "target": "vermoegenAbschlussJa",
            "cond": "below10kBuergergeld"
          },
          {
            "target": "vermoegenError",
            "cond": "above10k"
          },
          {
            "target": "erwerbstaetigkeit"
          }
        ],
        "BACK": { "target": "staatlicheLeistungen" }
      }
    },
    "vermoegenAbschlussJa": {
      "type": "final",
      "on": {
        "BACK": { "target": "vermoegen" }
      }
    },
    "vermoegenError": {
      "type": "final",
      "on": {
        "BACK": { "target": "vermoegen" }
      }
    },
    "erwerbstaetigkeit": {
      "on": {
        "SUBMIT": {
          "target": "partnerschaft"
        },
        "BACK": { "target": "vermoegen" }
      }
    },
    "partnerschaft": {
      "on": {
        "SUBMIT": {
          "target": "genauigkeit"
        },
        "BACK": { "target": "erwerbstaetigkeit" }
      }
    },
    "genauigkeit": {
      "on": {
        "SUBMIT": [
          {
            "target": "kinderKurz",
            "cond": "no"
          },
          {
            "target": "einkommen"
          }
        ],
        "BACK": { "target": "partnerschaft" }
      }
    },
    "kinderKurz": {
      "on": {
        "SUBMIT": [
          {
            "target": "kinderAnzahlKurz",
            "cond": "yes"
          },
          {
            "target": "verfuegbaresEinkommen"
          }
        ],
        "BACK": { "target": "genauigkeit" }
      }
    },
    "kinderAnzahlKurz": {
      "on": {
        "SUBMIT": {
          "target": "verfuegbaresEinkommen"
        },
        "BACK": { "target": "kinderKurz" }
      }
    },
    "einkommen": {
      "on": {
        "SUBMIT": [
          {
            "target": "einkommenPartner",
            "cond": "yes"
          },
          {
            "target": "kinder"
          }
        ],
        "BACK": { "target": "genauigkeit" }
      }
    },
    "verfuegbaresEinkommen": {
      "on": {
        "SUBMIT": [
          {
            "target": "verfuegbaresEinkommenAbschlussJa",
            "cond": "nonCriticalVerfuegbaresEinkommen"
          },
          {
            "target": "verfuegbaresEinkommenAbschlussVielleicht",
            "cond": "verfuegbaresEinkommen"
          },
          {
            "target": "verfuegbaresEinkommenAbschlussNein",
            "cond": "yes"
          }
        ],
        "BACK": [
          {
            "target": "kinderAnzahlKurz",
            "cond": "isPayingForKids"
          },
          { "target": "kinderKurz" }
        ]
      }
    },
    "verfuegbaresEinkommenAbschlussJa": {
      "type": "final",
      "on": {
        "BACK": {
          "target": "verfuegbaresEinkommen"
        }
      }
    },
    "verfuegbaresEinkommenAbschlussVielleicht": {
      "type": "final",
      "on": {
        "BACK": {
          "target": "verfuegbaresEinkommen"
        }
      }
    },
    "verfuegbaresEinkommenAbschlussNein": {
      "type": "final",
      "on": {
        "BACK": {
          "target": "verfuegbaresEinkommen"
        }
      }
    },
    "einkommenPartner": {
      "on": {
        "SUBMIT": {
          "target": "kinder"
        },
        "BACK": { "target": "einkommen" }
      }
    },
    "kinder": {
      "on": {
        "SUBMIT": [
          {
            "target": "kinderAnzahl",
            "cond": "yes"
          },
          {
            "target": "unterhalt"
          }
        ],
        "BACK": { "target": "einkommenPartner" }
      }
    },
    "kinderAnzahl": {
      "on": {
        "SUBMIT": {
          "target": "einkommenKinder"
        },
        "BACK": { "target": "kinder" }
      }
    },
    "einkommenKinder": {
      "on": {
        "SUBMIT": {
          "target": "unterhalt"
        },
        "BACK": { "target": "kinderAnzahl" }
      }
    },
    "unterhalt": {
      "on": {
        "SUBMIT": [
          {
            "target": "unterhaltSumme",
            "cond": "yes"
          },
          {
            "target": "miete"
          }
        ],
        "BACK": { "target": "einkommenKinder" }
      }
    },
    "unterhaltSumme": {
      "on": {
        "SUBMIT": {
          "target": "miete"
        },
        "BACK": { "target": "unterhalt" }
      }
    },
    "miete": {
      "on": {
        "SUBMIT": {
          "target": "weitereZahlungen"
        },
        "BACK": { "target": "unterhaltSumme" }
      }
    },
    "weitereZahlungen": {
      "on": {
        "SUBMIT": [
          {
            "target": "weitereZahlungenSumme",
            "cond": "yes"
          },
          {
            "target": "weitereZahlungenAbschlussVielleicht",
            "cond": "nonCritical"
          },
          {
            "target": "weitereZahlungenAbschlussNein",
            "cond": "incomeTooHigh"
          },
          {
            "target": "weitereZahlungenAbschlussJa"
          }
        ],
        "BACK": { "target": "miete" }
      }
    },
    "weitereZahlungenAbschlussVielleicht": {
      "type": "final",
      "on": {
        "BACK": {
          "target": "weitereZahlungen"
        }
      }
    },
    "weitereZahlungenAbschlussNein": {
      "type": "final",
      "on": {
        "BACK": {
          "target": "weitereZahlungen"
        }
      }
    },
    "weitereZahlungenAbschlussJa": {
      "type": "final",
      "on": {
        "BACK": {
          "target": "weitereZahlungen"
        }
      }
    },
    "weitereZahlungenSumme": {
      "on": {
        "SUBMIT": [
          {
            "target": "weitereZahlungenSummeAbschlussVielleicht",
            "cond": "nonCritical"
          },
          {
            "target": "weitereZahlungenSummeAbschlussNein",
            "cond": "incomeTooHigh"
          },
          {
            "target": "weitereZahlungenSummeAbschlussJa"
          }
        ],
        "BACK": { "target": "weitereZahlungen" }
      }
    },
    "weitereZahlungenSummeAbschlussVielleicht": {
      "type": "final",
      "on": {
        "BACK": {
          "target": "weitereZahlungenSumme"
        }
      }
    },
    "weitereZahlungenSummeAbschlussNein": {
      "type": "final",
      "on": {
        "BACK": {
          "target": "weitereZahlungenSumme"
        }
      }
    },
    "weitereZahlungenSummeAbschlussJa": {
      "type": "final",
      "on": {
        "BACK": {
          "target": "weitereZahlungenSumme"
        }
      }
    }
  }
}
