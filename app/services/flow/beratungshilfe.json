{
  "id": "Beratungshilfe",
  "initial": "rechtsschutzversicherung",
  "states": {
    "rechtsschutzversicherung": {
      "on": {
        "SUBMIT": [
          {
            "target": "wurdeVerklagt",
            "cond": "no"
          },
          {
            "target": "rechtsschutzversicherungError"
          }
        ]
      }
    },
    "rechtsschutzversicherungError": { "type": "final" },
    "wurdeVerklagt": {
      "on": {
        "SUBMIT": [
          {
            "target": "klageEingereicht",
            "cond": "no"
          },
          {
            "target": "wurdeVerklagtError"
          }
        ]
      }
    },
    "wurdeVerklagtError": { "type": "final" },
    "klageEingereicht": {
      "on": {
        "SUBMIT": [
          {
            "target": "hamburgOderBremen",
            "cond": "no"
          },
          {
            "target": "klageEingereichtError"
          }
        ]
      }
    },
    "klageEingereichtError": { "type": "final" },
    "hamburgOderBremen": {
      "on": {
        "SUBMIT": [
          {
            "target": "beratungshilfeBeantragt",
            "cond": "no"
          },
          {
            "target": "hamburgOderBremenError"
          }
        ]
      }
    },
    "hamburgOderBremenError": { "type": "final" },
    "beratungshilfeBeantragt": {
      "on": {
        "SUBMIT": [
          {
            "target": "eigeninitiative",
            "cond": "no"
          },
          {
            "target": "beratungshilfeBeantragtError"
          }
        ]
      }
    },
    "beratungshilfeBeantragtError": { "type": "final" },
    "eigeninitiative": {
      "on": {
        "SUBMIT": [
          {
            "target": "kostenfreieBeratung",
            "cond": "yes"
          },
          {
            "target": "eigeninitiativeWarnung"
          }
        ]
      }
    },
    "eigeninitiativeWarnung": {
      "on": {
        "SUBMIT": {
          "target": "kostenfreieBeratung"
        }
      }
    },
    "kostenfreieBeratung": {
      "on": {
        "SUBMIT": [
          {
            "target": "staatlicheLeistungen",
            "cond": "yes"
          },
          {
            "target": "kostenfreieBeratungWarnung"
          }
        ]
      }
    },
    "kostenfreieBeratungWarnung": {
      "on": {
        "SUBMIT": {
          "target": "staatlicheLeistungen"
        }
      }
    },
    "staatlicheLeistungen": {
      "on": {
        "SUBMIT": [
          {
            "target": "staatlicheLeistungenAbschlussJa",
            "cond": "staatlicheLeistung"
          },
          {
            "target": "vermoegen"
          }
        ]
      }
    },
    "staatlicheLeistungenAbschlussJa": { "type": "final" },
    "vermoegen": {
      "on": {
        "SUBMIT": [
          {
            "target": "vermoegenAbschlussJa",
            "cond": "below10kBuergergeld"
          },
          {
            "target": "vermoegenError",
            "cond": "above10k"
          },
          {
            "target": "erwerbstaetigkeit"
          }
        ]
      }
    },
    "vermoegenAbschlussJa": { "type": "final" },
    "vermoegenError": { "type": "final" },
    "erwerbstaetigkeit": {
      "on": {
        "SUBMIT": {
          "target": "partnerschaft"
        }
      }
    },
    "partnerschaft": {
      "on": {
        "SUBMIT": {
          "target": "genauigkeit"
        }
      }
    },
    "genauigkeit": {
      "on": {
        "SUBMIT": [
          {
            "target": "kinderKurz",
            "cond": "no"
          },
          {
            "target": "einkommen"
          }
        ]
      }
    },
    "kinderKurz": {
      "on": {
        "SUBMIT": [
          {
            "target": "kinderAnzahlKurz",
            "cond": "yes"
          },
          {
            "target": "verfuegbaresEinkommen"
          }
        ]
      }
    },
    "kinderAnzahlKurz": {
      "always": {
        "target": "verfuegbaresEinkommen"
      }
    },
    "einkommen": {
      "on": {
        "SUBMIT": [
          {
            "target": "einkommenPartner",
            "cond": "yes"
          },
          {
            "target": "kinder"
          }
        ]
      }
    },
    "verfuegbaresEinkommen": {
      "on": {
        "SUBMIT": [
          {
            "target": "verfuegbaresEinkommenAbschlussJa",
            "cond": "nonCriticalVerfuegbaresEinkommen"
          },
          {
            "target": "verfuegbaresEinkommenAbschlussVielleicht",
            "cond": "verfuegbaresEinkommen"
          },
          {
            "target": "verfuegbaresEinkommenAbschlussNein",
            "cond": "yes"
          }
        ]
      }
    },
    "verfuegbaresEinkommenAbschlussJa": { "type": "final" },
    "verfuegbaresEinkommenAbschlussVielleicht": { "type": "final" },
    "verfuegbaresEinkommenAbschlussNein": { "type": "final" },
    "einkommenPartner": {
      "on": {
        "SUBMIT": {
          "target": "kinder"
        }
      }
    },
    "kinder": {
      "on": {
        "SUBMIT": [
          {
            "target": "kinderAnzahl",
            "cond": "yes"
          },
          {
            "target": "unterhalt"
          }
        ]
      }
    },
    "kinderAnzahl": {
      "on": {
        "SUBMIT": {
          "target": "einkommenKinder"
        }
      }
    },
    "einkommenKinder": {
      "on": {
        "SUBMIT": {
          "target": "unterhalt"
        }
      }
    },
    "unterhalt": {
      "on": {
        "SUBMIT": [
          {
            "target": "unterhaltSumme",
            "cond": "yes"
          },
          {
            "target": "miete"
          }
        ]
      }
    },
    "unterhaltSumme": {
      "on": {
        "SUBMIT": {
          "target": "miete"
        }
      }
    },
    "miete": {
      "on": {
        "SUBMIT": {
          "target": "weitereZahlungen"
        }
      }
    },
    "weitereZahlungen": {
      "on": {
        "SUBMIT": [
          {
            "target": "weitereZahlungenSumme",
            "cond": "yes"
          },
          {
            "target": "weitereZahlungenAbschlussVielleicht",
            "cond": "nonCritical"
          },
          {
            "target": "weitereZahlungenAbschlussNein",
            "cond": "incomeTooHigh"
          },
          {
            "target": "weitereZahlungenAbschlussJa"
          }
        ]
      }
    },
    "weitereZahlungenAbschlussVielleicht": { "type": "final" },
    "weitereZahlungenAbschlussNein": { "type": "final" },
    "weitereZahlungenAbschlussJa": { "type": "final" },
    "weitereZahlungenSumme": {
      "on": {
        "SUBMIT": [
          {
            "target": "weitereZahlungenSummeAbschlussVielleicht",
            "cond": "nonCritical"
          },
          {
            "target": "weitereZahlungenSummeAbschlussNein",
            "cond": "incomeTooHigh"
          },
          {
            "target": "weitereZahlungenSummeAbschlussJa"
          }
        ]
      }
    },
    "weitereZahlungenSummeAbschlussVielleicht": { "type": "final" },
    "weitereZahlungenSummeAbschlussNein": { "type": "final" },
    "weitereZahlungenSummeAbschlussJa": { "type": "final" }
  }
}
