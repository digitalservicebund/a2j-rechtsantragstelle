    "storybook": "^10.2.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.2.7",
    "vite-env-only": "^3.0.3",
    "vite-node": "^5.3.0",
    "vite-tsconfig-paths": "^6.0.5",
